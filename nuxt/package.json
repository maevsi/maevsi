{
  "name": "maevsi",
  "private": true,
  "version": "0.85.0",
  "scheduleVersion": "0.64.3",
  "description": "The manager for events supported by invitees.",
  "scripts": {
    "creevey": "creevey --config ./.storybook/creevey/creevey.config.js",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:quiet": "yarn lint:js --quiet && yarn lint:style --quiet",
    "lint:fix": "yarn lint:js --fix && yarn lint:style --fix",
    "storybook": "STORYBOOK=true nuxt storybook -p 6006",
    "storybook:build": "STORYBOOK=true nuxt storybook build",
    "storybook:serve": "npx http-server ./storybook-static -p 6006",
    "storybook:static": "yarn storybook:build && yarn storybook:serve",
    "test": "jest && yarn test:visual",
    "test:visual": "(wget --spider ${HOST_IP:-127.0.0.1}:4444 || yarn selenium:start) && start-server-and-test 'yarn storybook:static' 6006 'sleep 5 && yarn creevey'",
    "selenium:start": "docker-compose --file ./.storybook/creevey/stack-selenium-grid.yml --project-directory .storybook/creevey --project-name selenium up -d",
    "selenium:stop": "docker-compose --file ./.storybook/creevey/stack-selenium-grid.yml --project-directory .storybook/creevey --project-name selenium down",
    "supportedBrowsers": "echo \"module.exports = $(browserslist-useragent-regexp --allowHigherVersions);\" > supportedBrowsers.js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
