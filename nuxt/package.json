{
  "name": "maevsi",
  "private": true,
  "version": "0.99.4",
  "scheduleVersion": "0.99.4",
  "description": "The manager for events supported by invitees.",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:quiet": "yarn lint:js --quiet && yarn lint:style --quiet",
    "lint:fix": "yarn lint:js --fix && yarn lint:style --fix",
    "storycap:base": "storycap http://localhost:6006 --viewport 640x1020 --viewport 1280x1020",
    "storycap:actual": "yarn storycap:base --outDir .storybook/storycap/images/actual",
    "storycap:expected": "yarn storycap:base --outDir .storybook/storycap/images/expected",
    "storycap:update": "sh ./.storybook/storycap/update.sh",
    "storybook": "cross-env STORYBOOK=true nuxt storybook -p 6006",
    "storybook:build": "cross-env STORYBOOK=true nuxt storybook build",
    "storybook:serve": "npx http-server ./storybook-static -p 6006",
    "storybook:static": "yarn storybook:build && yarn storybook:serve",
    "test": "yarn test:code && yarn test:visual",
    "test:code": "jest",
    "test:visual": "start-server-and-test 'yarn storybook --ci' 6006 'bash ./.storybook/storycap/test.sh'",
    "test:visual:update": "./.storybook/storycap/update.sh",
    "supportedBrowsers": "echo \"module.exports = $(browserslist-useragent-regexp --allowHigherVersions);\" > supportedBrowsers.js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
