{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "#app": [
        "node_modules/@nuxt/bridge/dist/runtime/index"
      ],
      "~/*": [
        "./*"
      ]
    },
    "types": [
      "@types/node",
      "@nuxt/types-edge",
      "@nuxtjs/color-mode",
      "@nuxtjs/google-analytics",
      "@nuxtjs/i18n",
      "@nuxtjs/moment",
      "jest",
      "node",
      "vue-apollo/types",
      "vue-clipboard2",
      "vue-sweetalert2",
    ],
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ],
  "extends": "./.nuxt/tsconfig.json",
  "vueCompilerOptions": {
    "target": 2.7,
  }
}
